{
    "name": "leonardocaldas/composable-query-builder",
    "type": "composer-plugin",
    "require": {
        "laravel/framework": "^11.0 || ^12.0",
        "php": "^8.2",
        "composer-plugin-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ComposableQueryBuilder\\": "src/"
        }
    },
    "extra": {
        "class": "ComposableQueryBuilder\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "leonardocaldas/composable-query-builder": true
        }
    },
    "authors": [
        {
            "name": "Leonardo Caldas",
            "email": "leonardo.ferreira.caldas@gmail.com"
        }
    ],
    "license": "MIT"
}